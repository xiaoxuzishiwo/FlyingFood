page {
  display: block;
  min-height: 100%;
  background-color: #f5f5f5;
}
::-webkit-scrollbar{width: 0;height: 0;color: transparent;}

.body{
  background: #ffffff;
  font-size: 32rpx;
}
.body view{
  height: 90rpx;
  padding: 0 25rpx;
  line-height: 90rpx;
  border-bottom: .5px solid #eae9ea;
  position: relative;
}
image{
  position: absolute;
  right: 40rpx;
  top: 25rpx;
  height: 40rpx;
  width: 30rpx;
  float: left;
}
